package chapter2.apples;

public class Apple {
	private int size;
	private String color;
	public int getSize() {
		return size;
	}
	public String getColor() {
		return color;
	}
	public Apple(int size, String color){
		this.size=size;
		this.color=color;
	}
	public String toString(){
		return "bla";
	}
	
}
